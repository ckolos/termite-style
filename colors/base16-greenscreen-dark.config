## Termite Styling Program
## Author : Aditya Shakya

[options]

allow_bold = true
audible_bell = true
browser = xdg-open
clickable_url = true
cursor_blink = system
cursor_shape = block
dynamic_title = true
filter_unmatched_urls = true
font =
geometry = 480x280
icon_name = terminal
modify_other_keys = false
mouse_autohide = false
scroll_on_output = false
scroll_on_keystroke = true
scrollback_lines = 15000
size_hints = false
search_wrap = true
urgent_on_bell = true
highlight = #30343e

[colors]

foreground=   #00bb00
background=   #001100
cursor=  #00bb00

color0=       #001100
color1=       #007700
color2=       #00bb00
color3=       #007700
color4=       #009900
color5=       #00bb00
color6=       #005500
color7=       #00bb00

